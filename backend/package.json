{
  "name": "Auth Boilerplate",
  "version": "1.0.0",
  "description": "by Marcus Peh",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json --format stylish",
    "build": "tsc",
    "tsc": "tsc && echo 'tsc-ed'",
    "serve": "ts-node src/server.ts",
    "start": "npm install && npm run migration && npm run serve",
    "typeorm": "typeorm-ts-node-esm -d ./src/data-source.ts",
    "create-migration": "npm run typeorm migration:generate -n ./src/migration/AddUserAndTokenTable",
    "migration": "npm run typeorm migration:run",
    "revert": "npm run tsc && ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.6",
    "@types/koa__cors": "^4.0.0",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-cookie": "^1.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^20.1.4",
    "@types/node-rsa": "^1.1.1",
    "@types/validator": "^13.7.17",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-cookie": "^1.0.0",
    "koa-router": "^12.0.0",
    "moment": "^2.29.4",
    "node-rsa": "^1.1.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typeorm": "^0.3.16",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": "16.x"
  }
}
